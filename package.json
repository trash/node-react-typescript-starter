    "webpack-dev-server": "^3.1.0"
  },
  "scripts": {
    "clean-client": "rm -rf client/build && rm -rf client/prod-build",
    "clean-server": "rm -rf server/build",
    "clean": "npm run clean-client && npm run clean-server",
    "build-client": "npm run webpack",
    "build-server": "node_modules/.bin/babel server --out-dir server/build --extensions '.ts,.js'",
    "build": "npm run build-server && npm run build-client",
    "serve": "node server/build/server",
    "start": "npm run serve",
    "local-start": "npm run build && npm run serve",
    "dev-server": "nodemon",
    "dev-client": "cd client && webpack-dev-server",
    "dev": "npm run dev-client & npm run dev-server",
    "test": "jest",
    "test-watch": "node --debug-brk ./node_modules/jest/bin/jest --watch --runInBand",
    "webpack": "node_modules/.bin/webpack --config client/webpack/webpack.dev.js"
  },
  "repository": {
    "type": "git",
    "url": "git://github.com/trash/yearinreview.git"
  },
  "license": "MIT",
  "devDependencies": {
    "@types/auth0-js": "^8.11.2",
    "@types/express-jwt": "^0.0.38",
    "@types/history": "^4.6.2",
    "@types/jest": "^19.2.3",
    "@types/moment": "^2.13.0",
    "@types/node": "^9.4.6",
    "@types/react-router": "^4.0.22",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "html-webpack-plugin": "^3.0.4",
    "jest": "^20.0.3",
    "less": "^3.0.1",
    "less-loader": "^4.1.0",
    "style-loader": "^0.20.3",
    "ts-jest": "^20.0.4",
    "ts-loader": "^4.0.1",
    "webpack-cli": "^2.0.10",
    "webpack-merge": "^4.1.2"
  },
  "jest": {
    "setupTestFrameworkScriptFile": "<rootDir>/app/test/setupTests.ts",
    "transform": {
      "^.+\\.tsx?$": "<rootDir>/node_modules/ts-jest/preprocessor.js"
    },
    "testRegex": "(/__tests__/.*|\\.(test|spec))\\.(ts|tsx|js)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "json"
    ]
  }
}
